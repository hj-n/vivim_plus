<idea-plugin>
  <version>1.0-SNAPSHOT</version>
  <idea-version since-build="192.6262" until-build="192.*"/>
  <id>project_Team7</id>
  <name>simpleIdeaVim</name>
  <vendor email="youremail@postech.ac.kr" url="http://csed332.postech.ac.kr">POSTECH CSED332</vendor>
  <description>
    CESD332 Project&lt;br&gt;
    &lt;em&gt;simpleIdeaVim&lt;/em&gt; is simple vim emulator with functions of some vim plugins.
    </description>
  <extensions defaultExtensionNs="com.intellij">
    <toolWindow id="Project Structure" anchor="right" factoryClass="project_Team7.MyToolWindowFactory"/>
    <editorTypedHandler id="vim command" implementationClass="project_Team7.MyTypedHandler"/>
  </extensions>
  <actions>
    <action id="project_Team7.commandBarAction" class="project_Team7.vimCommandBarAction" text="vim command" description="command bar action for vim plugin">
      <add-to-group group-id="ToolsMenu" anchor="first"/>
    </action>
  </actions>
</idea-plugin>
